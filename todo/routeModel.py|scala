# http://wiki.openstreetmap.org/wiki/Open_Source_Routing_Machine
import pandas
import os
import requests

trips = pandas.DataFrame.read_csv(os.path.expanduser('~/Desktop/maps/trips.csv')
routes = []

for trip in trips:
  r = requests.get('http://localhost/query?loc=' trip)
r.json()['remote_addr']

  routes.append()
  
